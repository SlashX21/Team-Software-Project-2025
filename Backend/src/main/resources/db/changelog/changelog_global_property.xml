<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

        <!-- Define type mapping -->
        <!-- For example, map 'string' type to 'VARCHAR(255)' in MySQL -->
        <property name="type.string" value="VARCHAR(255)" dbms = "mysql"/>
        <property name="type.longtext" value="TEXT" dbms = "mysql"/>
        <property name="type.int" value="INT" dbms = "mysql"/>
        <property name="type.datetime" value="DATETIME" dbms = "mysql"/>
        <property name="type.timestamp" value="TIMESTAMP" dbms = "mysql"/>
        <property name="type.boolean" value="BOOLEAN" dbms = "mysql"/>
        <property name="type.double" value="DOUBLE" dbms = "mysql"/>
        <property name="type.float" value="FLOAT" dbms = "mysql"/>
        <property name="type.decimal" value="DECIMAL" dbms = "mysql"/>
        <property name="type.date" value="DATE" dbms="mysql"/>

</databaseChangeLog>
